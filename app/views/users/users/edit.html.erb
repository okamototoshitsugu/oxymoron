<h1>登録情報変更</h1>
<%= link_to "退会する", users_users_unsubscribe_path %>

<%= form_for @user, url:users_user_path(@user.id), method: :patch do |f| %>
<%= f.label :MAME, "NAME" %>
<%= f.text_field :name %><br />
<%= f.label :EMAIL, "EMAIL" %>
<%= f.text_field :email %><br />
<%= f.submit "編集内容を保存する" %>
<% end %>
